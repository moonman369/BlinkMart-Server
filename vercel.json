{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/user/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/category/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/subcategory/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/product/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/cart/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/address/(.*)",
      "dest": "/index.js"
    }
  ]
}
